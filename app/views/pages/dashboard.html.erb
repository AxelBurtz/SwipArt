<% if params[:tab] == "match" || params[:tab].nil?  %>
  <%= render "dashboard_exhibition_recommanded" %>
<% end %>

<% if params[:tab] == "booked" %>
  <%= render "dashboard_exhibition_booked", user: @user %>
<% end %>

<% if params[:tab] == "done" %>
  <%= render "dashboard_my_exhibition_done" %>
<% end %>
